# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/pricing_service
DB_HOST=localhost
DB_PORT=5432
DB_NAME=pricing_service
DB_USER=postgres
DB_PASSWORD=password

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Product Feature Flags
ENABLE_TERM_LIFE=true
ENABLE_WHOLE_LIFE=false
ENABLE_ANNUITIES=false

# Default Settings
DEFAULT_PRODUCT_TYPE=term_life
QUOTE_EXPIRY_HOURS=720

# Rate Table Versions
RATE_TABLE_VERSION_TERM_LIFE=v1.0.0

# Security
JWT_SECRET=your_jwt_secret_here
API_RATE_LIMIT=1000

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090